---
import Layout from '../../layouts/Layout.astro';
import Caption from '../../components/Caption.astro';
import Img from '../../components/Img.astro';
---

<Layout title="35">
        <Caption>
            <h2>Bellows alphabet typography</h2>
            <p>Independent production<br>
            Graphic design : TAMURA Ikuho<br>
            2023</p>
            <p>-</p>
        </Caption>
        <Img>
            <img src="../../public/asset/img/35.webp" alt="Image 35">
        </Img>
        <Img>
            <img src="../../public/asset/img/35-2.webp" alt="Image 35-2">
        </Img>
        <Img>
            <img src="../../public/asset/img/35-3.webp" alt="Image 35-3">
        </Img>
        <Img>
            <img src="../../public/asset/img/35-4.webp" alt="Image 35-4">
        </Img>
</Layout>

<style>    
    img {
        width: 65vw;
        margin: auto;
        display: block;
    }

    @media screen and (max-width: 560px){
        img {
        width: 90vw;

        }
    }
</style>